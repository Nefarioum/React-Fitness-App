{
  "name": "fitness-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:tailwind": "tailwindcss --input tailwind/input.css --output tailwind/tailwind.css --no-autoprefixer && tailwind-rn -i tailwind/tailwind.css",
    "dev:tailwind": "concurrently \"tailwindcss --input tailwind/input.css --output tailwind/tailwind.css --no-autoprefixer --watch\" \"tailwind-rn -i tailwind/tailwind.css --watch\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@sentry/react-native": "^3.3.1",
    "expo": "~44.0.0",
    "expo-application": "~4.0.1",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.7",
    "firebase": "9.6.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-dotenv": "^3.3.1",
    "react-native-elements": "^3.4.2",
    "react-native-google-places-autocomplete": "^2.4.1",
    "react-native-maps": "0.29.4",
    "react-native-maps-directions": "^1.8.0",
    "react-native-toast-message": "^2.1.1",
    "react-native-web": "0.17.1",
    "react-redux": "^7.2.6",
    "sentry-expo": "^4.0.0",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.3.5"
  },
  "private": true
}
